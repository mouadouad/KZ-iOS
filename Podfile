  target 'KzTests' do
    inherit! :search_paths
    # Pods for testing
pod 'Firebase/Database'
pod 'Firebase/Analytics'
pod 'Google-Mobile-Ads-SDK'
  end

  target 'KzUITests' do
    # Pods for testing
pod 'Firebase/Database'
pod 'Firebase/Analytics'
pod 'Google-Mobile-Ads-SDK'
  end

end
